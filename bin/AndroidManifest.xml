            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.quickplay.QuickbaseActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        
         <activity
            android:name="com.quickplay.MessagingActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>        
        <activity android:name=".MessagingActivity" android:screenOrientation="portrait"/>
        <activity android:name="changefragment.FragmentChangeActivity" />
        
        

